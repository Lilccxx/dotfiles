keycode 111 = Up Prior Up
keycode 116 = Down Next Down
keycode 113 = Left NoSymbol Left
keycode 114 = Right NoSymbol Right

! replace capslock with ctrl
!clear lock
!keycode 0xffe5 = Control_R
! add Control = Control_R
remove Lock = Caps_Lock
remove Control = Control_L
keysym Control_L = Caps_Lock
keysym Caps_Lock = Control_L
add Lock = Caps_Lock
add Control = Control_L

! multimedia keys
! mute
keycode 121 = F10 F10 F10 F10 XF86Switch_VT_10
keycode 76 = XF86AudioMute F10 F10 XF86Switch_VT_10
! volume down
keycode 122 = F11 F11 F11 F11 XF86Switch_VT_11
keycode 95 = XF86AudioLowerVolume F11 F11 XF86Switch_VT_11
! volume up
keycode 123 = F12 F12 F12 F12 XF86Switch_VT_12
keycode 96 = XF86AudioRaiseVolume F12 F12 XF86Switch_VT_12
! keyboard brightness down
keycode 237 = F3 F3 F3 F3 XF86Switch_VT_3
keycode 69 = XF86KbdBrightnessDown F3 F3 XF86Switch_VT_3
! keyboard brightness up
keycode 238 = F4 F4 F4 F4 XF86Switch_VT_4
keycode 70 = XF86KbdBrightnessUp F4 F4 XF86Switch_VT_4
